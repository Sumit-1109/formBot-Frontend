{
  "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ],
  "functions": {
    "source": "src"
  },
  "env": {
    "VERCEL_BUILDER_IGNORE_FILE": "**/node_modules/**"
  },
  "cache": {
    "includes": [
      "src/**/*"
    ]
  }
}
